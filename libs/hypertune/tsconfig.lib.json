{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "outDir": "../../dist/libs/hypertune",
    "tsBuildInfoFile": "../../dist/libs/hypertune/tsconfig.lib.tsbuildinfo",
    "module": "es2022",
    "lib": ["es2022", "dom"],
    "jsx": "react-jsx",
    "types": ["node", "react", "react-dom"],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "references": []
}
