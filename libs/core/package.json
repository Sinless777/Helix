{
  "name": "@helix-ai/core",
  "version": "0.0.1",
  "private": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "helix",
    "core",
    "shared"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auth/mikro-orm-adapter": "^2.11.1",
    "@infisical/sdk": "^4.0.6",
    "@vercel/edge-config": "^1.4.3",
    "hypertune": "^2.10.0",
    "next-auth": "^4.24.13",
    "server-only": "0.0.1",
    "yaml": "^2.2.1"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./infisical": {
      "import": "./src/infisical.ts",
      "types": "./src/infisical.ts",
      "default": "./src/infisical.ts"
    },
    "./config": {
      "import": "./src/config.index.ts",
      "types": "./src/config.index.ts",
      "default": "./src/config.index.ts"
    },
    "./config/*": {
      "import": "./src/config/*.ts",
      "types": "./src/config/*.ts",
      "default": "./src/config/*.ts"
    },
    "./hypertune": {
      "import": "./src/hypertune/index.ts",
      "types": "./src/hypertune/index.ts",
      "default": "./src/hypertune/index.ts"
    },
    "./hypertune/*": {
      "import": "./src/hypertune/*.ts",
      "types": "./src/hypertune/*.ts",
      "default": "./src/hypertune/*.ts"
    }
  }
}
